export default function ForEscape() {
	let things = ["a", "b", "c", "d", "e"]
	@render {
		<section>
			@for (let i = 0; i < 5; i++) {
				<p>{i}</p>
				<div data-testid="input1-{i}" name={i} />
				<div data-testid="input2-{i}" name="{i}" />
				<div data-testid="input3-{i}" name={things[i]} />
				<input :value={i} name="{i}" />
			}
		</section>
	}
}
