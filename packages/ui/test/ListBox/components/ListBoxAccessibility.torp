import { ListBox, ListBoxItem } from "../../../src/ListBox/index";

export default function ListBoxAccessibility($props: any) {
	@render {
		<ListBox {$props.value} type="multiple" ariaLabel="The accessible listbox">
			<ListBoxItem value={0}>
				Role Content
			</ListBoxItem>
			<ListBoxItem value={1}>
				Selected Content
			</ListBoxItem>
			<ListBoxItem value={2}>
				Unselected Content
			</ListBoxItem>
			<ListBoxItem value={3}>
				Region Content
			</ListBoxItem>
			<ListBoxItem value={4} disabled>
				Disabled Content
			</ListBoxItem>
		</ListBox>
	}
}
