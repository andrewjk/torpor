{
	"name": "@tera/build",
	"version": "0.0.1",
	"description": "A framework for building sites and apps with Tera",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./response": {
			"types": "./dist/response.d.ts",
			"import": "./dist/response.js",
			"require": "./dist/response.cjs"
		},
		"./state": {
			"types": "./dist/state.d.ts",
			"import": "./dist/state.js",
			"require": "./dist/state.cjs"
		}
	},
	"private": true,
	"bin": {
		"tb": "./dist/bin/index.js"
	},
	"scripts": {
		"build": "tsup",
		"build:watch": "tsup --watch"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@tera/unplugin": "workspace:^",
		"@tera/view": "workspace:^",
		"vinxi": "^0.5.1"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"lilconfig": "^3.1.3",
		"tsup": "^8.3.5"
	}
}
