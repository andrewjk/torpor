export default function IsAvailable() {
	let $state = $watch({
		isAvailable: false
	});

	@render {
		<div>
			<div>{$state.isAvailable ? "Available" : "Not available"}</div>

			<input id="is-available" type="checkbox" &checked={$state.isAvailable} />
			<label for="is-available">Is available</label>
		</div>
	}
}
